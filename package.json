{
  "name": "csv-to-json",
  "version": "1.0.0",
  "description": "Commandline application that takes in CSV as input and converts it to JSON",
  "main": "index.js",
  "type": "commonjs",
  "bin": {
    "csvToJson": "./dist/index.js"
  },
  "scripts": {
    "test": "vitest",
    "dev": "npx ts-node ./src/index.ts",
    "debug": "node --inspect-brk $(npx -y which ts-node) ./src/index.ts",
    "build": "tsc"
  },
  "keywords": [
    "csv",
    "json"
  ],
  "author": "Edvinas",
  "license": "ISC",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^22.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "dependencies": {
    "better-sqlite3": "^11.5.0"
  }
}
